package com.trainbookingapp;

public class Seat {
    private String seatNum;

    public Seat(String seatNum) {
        this.seatNum = seatNum;
    }

    public String getSeatNum() {
        return seatNum;
    }

    public void setSeatNum(String seatNum) {
        this.seatNum = seatNum;
    }

    @Override
    public String toString() {
        return "" + seatNum;
    }
}
